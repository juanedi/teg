server: find `which teg` | entr -r teg --server
api_codegen: find `which teg` | entr -r teg --codegen
watch_haskell: stack build --file-watch
watch_elm_home: cd ui && elm-live --no-server -- src/Homepage.elm --output=static/_build/home.js --debug
watch_elm_game: cd ui && elm-live --no-server -- src/Main.elm --output=static/_build/game.js --debug
